{
  "name": "visol/google-cloud-storage",
  "description": "GoogleCloudStorage integration in TYPO3.",
  "type": "typo3-cms-extension",
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": "^11",
    "google/cloud-storage": "^1",
    "guzzlehttp/psr7": "^2",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-zlib": "*"
  },
  "autoload": {
    "psr-4": {
      "Visol\\GoogleCloudStorage\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "google_cloud_storage"
    }
  }
}
